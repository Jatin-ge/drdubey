// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model patients {
  id String @id @default(uuid()) @map("_id") @db.ObjectId
  NAME String? 
  AGE Int?
  SEX String?
  ADDRESS String?
}